{
  "name": "@flayyer/cli",
  "description": "Flayyer CLI",
  "version": "1.15.0",
  "author": "Patricio LÃ³pez Juri @lopezjurip",
  "repository": "flayyer/flayyer-cli",
  "license": "MIT",
  "private": false,
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "bin": {
    "flayyer": "./bin/run"
  },
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc.js",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "echo NO TESTS",
    "lint": "eslint '*/**/*.{js,ts,tsx}'",
    "version": "oclif-dev readme && git add README.md",
    "graphql:codegen": "apollo client:codegen --endpoint http://localhost:9001/graphql --target typescript --outputFlat --no-addTypename --includes src/flayyer-graphql/**/*.ts --tagName gql src/flayyer-graphql/types.ts"
  },
  "bugs": "https://github.com/flayyer/flayyer-cli/issues",
  "engines": {
    "node": ">=10.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/yarn.lock",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/flayyer/flayyer-cli",
  "keywords": [
    "oclif",
    "flayyer"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "oclif": {
    "commands": "./lib/commands",
    "bin": "flayyer",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "dependencies": {
    "@flayyer/flayyer": "^2.3.1",
    "@flayyer/goerr": "^1.2.0",
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^3.2.2",
    "archiver": "^5.3.0",
    "chalk": "^4.1.0",
    "chokidar": "^3.5.1",
    "cross-fetch": "^3.1.4",
    "debug": "^4.3.1",
    "dedent": "^0.7.0",
    "del": "^6.0.0",
    "form-data": "^4.0.0",
    "graphql": "^15.5.0",
    "graphql-request": "3.4.0",
    "mini-svg-data-uri": "^1.2.3",
    "open": "~7.4.2",
    "parcel-bundler": "^1.12.5",
    "qs": "^6.10.1",
    "tslib": "^2.2.0",
    "twemoji": "^13.0.2"
  },
  "devDependencies": {
    "@flayyer/eslint-config": "^1.0.6",
    "@oclif/dev-cli": "^1.26.0",
    "@types/archiver": "^5.1.0",
    "@types/debug": "^4.1.5",
    "@types/dedent": "^0.7.0",
    "@types/node": "^14.14.41",
    "@types/node-fetch": "^2.5.10",
    "@types/parcel-bundler": "^1.12.3",
    "apollo": "^2.32.5",
    "eslint": "^7.24.0",
    "eslint-config-oclif": "^3.1",
    "eslint-config-oclif-typescript": "^0.2.0",
    "globby": "^11.0.3",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  }
}
