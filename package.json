{
  "name": "@flayyer/cli",
  "description": "Flayyer CLI",
  "version": "1.9.1",
  "author": "Patricio LÃ³pez Juri @lopezjurip",
  "repository": "flayyer/flayyer-cli",
  "license": "MIT",
  "private": false,
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "bin": {
    "flayyer": "./bin/run"
  },
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc.js",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "echo NO TESTS",
    "lint": "eslint '*/**/*.{js,ts,tsx}'",
    "version": "oclif-dev readme && git add README.md"
  },
  "bugs": "https://github.com/flayyer/flayyer-cli/issues",
  "engines": {
    "node": ">=10.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/yarn.lock",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/flayyer/flayyer-cli",
  "keywords": [
    "oclif",
    "flayyer"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "oclif": {
    "commands": "./lib/commands",
    "bin": "flayyer",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^3.2.0",
    "archiver": "^5.1.0",
    "chalk": "^4.1.0",
    "chokidar": "^3.4.3",
    "cross-fetch": "^3.0.6",
    "debug": "^4.3.1",
    "dedent": "^0.7.0",
    "del": "^6.0.0",
    "form-data": "^3.0.0",
    "graphql": "^15.4.0",
    "graphql-request": "3.3.0",
    "parcel-bundler": "^1.12.4",
    "qs": "^6.9.4",
    "tslib": "^2.0.3"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.0",
    "@types/archiver": "^5.1.0",
    "@types/debug": "^4.1.5",
    "@types/dedent": "^0.7.0",
    "@types/node": "^14.14.13",
    "@types/node-fetch": "^2.5.7",
    "@types/parcel-bundler": "^1.12.1",
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "@typescript-eslint/parser": "^4.9.1",
    "eslint": "^7.15.0",
    "eslint-config-oclif": "^3.1",
    "eslint-config-oclif-typescript": "^0.2.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-prettier": "^3.2.0",
    "globby": "^11.0.1",
    "husky": "^4.3.5",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  }
}
